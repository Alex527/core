using System;
using System.Collections.Generic;

using Do.PluginLib;

namespace MyNamespace
{

	public class MyItem : IItem
	{
		public const string ItemType = "MyItem";
		
		protected string name, icon;
		
		public MyItem ()
		{
		}
		
		public string Type {
			get { return MyItem.ItemType; }
		}
		
		public string Name {
			get { return name; }
		}
		
		public string Icon {
			get { return icon; }
		}
	}
}
